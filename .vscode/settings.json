{
  //java formatter
  "java.format.settings.url": "java-formatter.xml",
  //java null analysis
  "java.compile.nullAnalysis.mode": "automatic",
  //java formatter
  "[java]": {
    //editor detect indentation
    "editor.detectIndentation": false
  },
  //java checkstyle configuration
  "java.checkstyle.configuration": "checkstyle-checks.xml",
  //java checkstyle version
  "java.checkstyle.version": "10.21.2",
  //format on save
  "editor.formatOnSave": true,
  //java update build configuration
  "java.configuration.updateBuildConfiguration": "automatic",
  //coverage gutter showLine true
  "coverage-gutters.showLineCoverage": true,
  //coverage file names
  "coverage-gutters.coverageFileNames": [
    "lcov.info",
    "cov.xml",
    "coverage.xml",
    "cobertura.xml",
    "jacoco.xml",
    "coverage.cobertura.xml",
    "jacocoTestReport.xml"
  ],
  //color of coverage gutters highlight
  "coverage-gutters.highlightlight": "rgba(0, 255, 0, 1)",
  // written below is not needed, and change encoding to UTF-8 is another way 
  // when your erea is China or Japan for example, 
  // you need to change you windows system settings to change encoding 
  // (Beta version: use Unicode UTF-8 to provide global language support)

  // "terminal.integrated.profiles.windows": {
